@model Alphabet

<form asp-action="Delete" asp-controller="Alphabet" method="post">
    <h3>Are you sure to delete this alphabet?</h3>
    <input type="hidden" asp-for="AlphabetId" value="@Model.AlphabetId"/>

    <button class="btn btn-success" type="submit">Yes</button>
    <a asp-action="AllAlphabets" asp-controller="Alphabet" class="btn btn-danger">No</a>
</form>
